<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd"
       default-lazy-init="true" default-autowire="constructor">

    <bean id="googleMapsAdapter" class="lt.tlistas.core.google_maps.plugin.GoogleMapsGeocodingGatewayAdapter"/>
    <bean id="googleMapsContextFactory" class="lt.tlistas.core.google_maps.plugin.GoogleMapsApiContextFactory"/>
    <bean id="googleMapsApi" class="lt.tlistas.core.google_maps.plugin.GoogleMapsGeocodingApi"/>
    <bean id="context" class="com.google.maps.GeoApiContext.Builder"/>

</beans>
