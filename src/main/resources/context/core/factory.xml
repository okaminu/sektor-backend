<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-5.0.2.xsd"
       default-lazy-init="true" default-autowire="constructor">

    <bean id="companyFactory" class="lt.tlistas.core.factory.CompanyFactory"/>
    <bean id="collaboratorFactory" class="lt.tlistas.core.factory.CollaboratorFactory"/>
    <bean id="countryFactory" class="lt.tlistas.core.factory.CountryFactory"/>
    <bean id="addressFactory" class="lt.tlistas.core.factory.AddressFactory"/>
    <bean id="customerFactory" class="lt.tlistas.core.factory.CustomerFactory"/>
    <bean id="projectFactory" class="lt.tlistas.core.factory.ProjectFactory"/>
    <bean id="userFactory" class="lt.tlistas.core.factory.UserFactory"/>
</beans>

