<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd"
       default-lazy-init="true" default-autowire="constructor">

    <bean id="collaboratorService" class="lt.tlistas.core.service.CollaboratorService"/>
    <bean id="requestService" class="lt.tlistas.crowbar.service.RequestService"/>
    <bean id="confirmationService" class="lt.tlistas.crowbar.service.ConfirmationService"/>
    <bean id="companyService" class="lt.tlistas.core.service.CompanyService"/>
    <bean id="customerService" class="lt.tlistas.core.service.CustomerService"/>
    <bean id="countryService" class="lt.tlistas.core.service.CountryService"/>
    <bean id="dateTimeProvider" class="lt.tlistas.core.service.time.LocalDateTimeProvider"/>
    <bean id="locationWorkLogService" class="lt.tlistas.core.service.LocationWorkLogService"/>
    <bean id="projectService" class="lt.tlistas.core.service.ProjectService"/>
    <bean id="userService" class="lt.tlistas.core.service.UserService"/>
    <bean id="validatorToExceptionConverter" class="lt.tlistas.core.exception.converter.ValidatorToExceptionConverter"/>
    <bean id="workLogService" class="lt.tlistas.core.service.WorkLogService"/>
</beans>

